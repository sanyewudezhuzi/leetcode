package main

import "fmt"

func twoSum(nums []int, target int) []int {
	hashMap := map[int]int{}
	for k, v := range nums {
		if i, ok := hashMap[target-v]; ok {
			return []int{k, i}
		}
		hashMap[v] = k
	}
	return nil
}

func main() {
	nums := []int{2, 7, 11, 15}
	target := 9
	fmt.Println(twoSum(nums, target))
}
