func addTwoNumbers(l1 *ListNode, l2 *ListNode) *ListNode {
	var ans *ListNode = &ListNode{}
	temp := ans
	var sum int
	for l1 != nil || l2 != nil || sum != 0 {
		if l1 != nil {
			sum += l1.Val
			l1 = l1.Next
		}
		if l2 != nil {
			sum += l2.Val
			l2 = l2.Next
		}
		temp.Next = &ListNode{Val: sum % 10}
		sum /= 10
		temp = temp.Next
	}
	return ans.Next
}
